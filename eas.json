{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "config": "preview",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "production": {
      "android": {
        "autoIncrement": true,
        "config": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}