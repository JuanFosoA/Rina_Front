{
  "expo": {
    "name": "rina-front",
    "slug": "rina",
    "scheme": "menu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rina.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rina.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juanfosoa.rina",

      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9442137b-28d6-49bf-a797-ec474b2e89a4"
      }
    },
    "owner": "juanfosoa"
  }
}
